{
  "compileOnSave": true,
  "compilerOptions": {
    "allowJs": false,
    "module": "commonjs",
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": false,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "target": "es5"
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "typeAcquisition": {
    "enable": true
  }
}
