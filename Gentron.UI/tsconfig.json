{
  "compileOnSave": true,
  "compilerOptions": {
    "allowJs": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": false,
    "target": "es5",
  },
  "exclude": [
    "node_modules",
    "dist",
    //"webpack.config.ts"
  ],
  "typeAcquisition": {
    "enable": true 
  }
}
